.pagination-box {
  position: relative;
  width: 98%;
  margin: 10px auto;
  height: 50px;
}
.pagination-box .pagination {
  position: absolute;
  width: auto;
  right: 0px;
  height: 50px;
}
.pagination-box .pagination ul {
  margin-top: 10px;
  width: 100%;
  height: 30px;
}
.pagination-box .pagination ul li {
  height: 30px;
  display: inline-block;
  margin: 0 3px;
}
.pagination-box .pagination ul li span {
  display: block;
  width: 28px;
  height: 28px;
  border: 1px solid #9b9b9b;
  line-height: 28px;
  text-align: center;
  font-size: 14px;
  color: #555;
  cursor: pointer;
  border-radius: 5px;
}
.pagination-box .pagination ul li span:hover {
  border: 1px solid #2475f8;
  color: #2475f8;
}
.pagination-box .pagination ul li span .iconfont {
  font-size: 14px;
}
.pagination-box .pagination ul li label {
  width: 200px;
  display: block;
}
.pagination-box .pagination ul li label input {
  width: 30px;
}
.pagination-box .pagination ul li label button {
  width: 50px;
  outline: 0;
  background: #3a8ee6;
  border: 0px;
  color: #fff;
  cursor: pointer;
}
.pagination-box .pagination ul .activeLi span {
  border: 1px solid #136cfb;
  color: #116cfd;
}
.pagination-box .pagination ul .five-page {
  text-align: center;
  font-size: 20px;
  cursor: pointer;
}
